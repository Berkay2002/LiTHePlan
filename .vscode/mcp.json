{
  "servers": {
    "github": {
      "url": "https://api.githubcopilot.com/mcp/",
      "type": "http"
    },
    "next-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "next-devtools-mcp@latest"]
    },
    "vercel": {
      "url": "https://mcp.vercel.com",
      "type": "http"
    },
    "sentry": {
      "url": "https://mcp.sentry.dev/mcp",
      "type": "http"
    },
    "ultracite": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://www.ultracite.ai/api/mcp/mcp"]
    }
  },
  "inputs": []
}
